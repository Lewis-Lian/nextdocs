{
  "name": "nextdocs",
  "version": "0.0.1",
  "description": "The minimal setup for nextdocs",
  "keywords": [
    "nextdocs",
    "Docs"
  ],
  "homepage": "https://wiki.l615.com",
  "repository": "github:Lewis/nextdocs",
  "license": "MIT",
  "author": "Lewis-Lian",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "zshy",
    "dev": "bun scripts/dev.ts",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-rsc": "^0.4.32",
    "commander": "^14.0.1",
    "fumadocs-core": "workspace:*",
    "fumadocs-mdx": "workspace:*",
    "fumadocs-ui": "workspace:*",
    "picocolors": "^1.1.1",
    "react-router": "^7.9.3",
    "tinyglobby": "^0.2.15",
    "vite": "^7.1.9",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "eslint-config-custom": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsconfig": "workspace:*",
    "typescript": "^5.9.3",
    "zshy": "^0.4.2"
  },
  "zshy": {
    "cjs": false,
    "module": "./src/index.ts",
    "bin": {
      "fumapress": "./src/cli/index.ts"
    },
    "exports": {
      ".": "./src/index.ts",
      "./*": "./src/*",
      "./vite": "./src/vite/index.ts"
    }
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js"
    }
  },
  "bin": {
    "fumapress": "./dist/cli/index.js"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
